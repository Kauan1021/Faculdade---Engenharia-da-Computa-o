/* Dados dois números inteiros, faça um programa em C para mostrar qual número é maior ! */
#include<stdio.h>
#include<stdlib.h>
#include<locale.h>
int main(){
 
   setlocale(LC_ALL,"portuguese");
   int num1, num2;
   
   printf("Informe um número inteiro n1 = ");
   scanf("%d",&num1);
   printf("Informe um número inteiro n2 = ");
   scanf("%d",&num2);
   //estrutura condicional simples
   if(num1 > num2){
            printf("O némero n1 = %d é maior !", num1);
}
   if(num1 == num2){
          printf("Os dois números (n = %d e n2 = %d) são iguais!!!", num1, num2);
}
