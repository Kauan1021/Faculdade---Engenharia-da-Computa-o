#include<stdio.h>
#include<stdlib.h>
#include<locale.h>
int main(){
    
    setlocale(LC_ALL,"portuguese");
    int cont = 1;
    while(cont <= 20){ // enquanto cont <= 20 faÃ§a3
               printf("valor do contador = %d : ", cont);
               printf("Seja bem vinda, Linguagem C!\n");
               cont++; //atualiza contador: cont = cont + 1;     
    }
    printf("\nFim do programa!\n");   
            
    system("pause");
    return 0;
}

